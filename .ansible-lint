exclude_paths:
  - ansible/group_vars/all/tanium_secrets.yml
  - ansible/credentials/
  - ansible/inventory/

skip_list:
  - fqcn
  - no-changed-when
  - risky-shell-pipe
  - var-naming
  - command-instead-of-module
  - command-instead-of-shell
  - risky-file-permissions
  - yaml
  - name
  - ignore-errors
  - internal-error
